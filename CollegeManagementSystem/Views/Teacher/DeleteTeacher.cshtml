@model Teacher 
@{
    ViewData["Title"] = "Delete Teacher";
}
<div class="text-center">
    <h3>Are you sure you want to delete teacher @Model.name?</h3>

</div>
<a class="btn btn-outline-primary btn-block" role="button" asp-controller="Teacher" asp-action="IndexTeacher">Cancel</a>
<a class="btn btn-outline-danger btn-block" role="button">Delete</a>
