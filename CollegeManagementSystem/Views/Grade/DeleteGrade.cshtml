@model Grade 
@{
    ViewData["Title"] = "Delete Grade";
}
<div class="text-center">
    <h3>Are you sure you want to delete @Model.record grade for @Model.student?</h3>

</div>
<a class="btn btn-outline-primary btn-block" role="button" asp-controller="Grade" asp-action="IndexGrade">Cancel</a>
<a class="btn btn-outline-danger btn-block" role="button" asp-route-id="@Model.Id" asp-controller="Grade" asp-action="GradeExclusion">Delete</a>
